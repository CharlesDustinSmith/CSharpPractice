<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <RootNamespace>WPF_ZooManager</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="WPF ZooManager\PanjutorialsDBDataSet.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>PanjutorialsDBDataSet.xsd</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="WPF ZooManager\PanjutorialsDBDataSet.xsd">
      <Generator>MSDataSetGenerator</Generator>
      <LastGenOutput>PanjutorialsDBDataSet.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
